{
    "name": "aerni/imagekit",
    "description": "Easily generate ImageKit URLs",
    "keywords": [
        "statamic",
        "imagekit"
    ],
    "homepage": "https://github.com/aerni/statamic-imagekit",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Aerni",
            "email": "hello@michaelaerni.ch",
            "homepage": "https://www.michaelaerni.ch",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aerni\\Imagekit\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "statamic": {
            "name": "Imagekit",
            "description": "Easily generate ImageKit URLs"
        },
        "laravel": {
            "providers": [
                "Aerni\\Imagekit\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
